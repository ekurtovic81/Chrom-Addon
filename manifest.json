{
  "manifest_version": 3,
  "name": "Export/Import History & Bookmarks",
  "version": "1.0.0",
  "description": "Export and import your browser history and bookmarks with timestamps and folder structure preserved",
  "permissions": [
    "history",
    "bookmarks",
    "downloads",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon128.png",
      "32": "icons/icon128.png",
      "48": "icons/icon128.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon128.png",
    "32": "icons/icon128.png",
    "48": "icons/icon128.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
